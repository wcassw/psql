postgres=# CREATE DATABASE "PSQLDB";

postgres=# create database PSQLDB with tablespace ts_plsql;

postgres# CREATE DATABASE "PSQLDB"
WITH TABLESPACE ts_plsql
OWNER "postgres"
ENCODING 'UTF8'
LC_COLLATE = 'en_US.UTF-8'
LC_CTYPE = 'en_US.UTF-8'
TEMPLATE template0;

-- View database information:
postgres=# \l

postgres# select * from pg_database;
